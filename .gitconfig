[credential]
	helper = osxkeychain
[user]
	name = Aaron Walker
	email = aaron@treasuryprime.com
[push]
	default = current
    autoSetupRemote = true
[core]
	excludesfile = /Users/aaronwalker/.gitignore
	pager = less -F -X
[init]
	defaultBranch = main
[pull]
	default = current
[core]
    pager = delta

[interactive]
    diffFilter = delta --color-only

[delta]
    navigate = true    # use n and N to move between diff sections
    light = false      # set to true if you're in a terminal w/ a light background color (e.g. the default macOS terminal)

[merge]
    conflictstyle = diff3
    tool = vimdiff

[diff]
    colorMoved = default
    tool = vimdiff
